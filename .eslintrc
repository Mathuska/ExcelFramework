{
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "requireConfigFile": false
      },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "google"
    ],
    "rules": {
        "require-jsdoc": [
            "error",
            {
             "require": {
               "FunctionDeclaration": false,
                "MethodDefinition": true,
                "ClassDeclaration": true,
                "ArrowFunctionExpression": true,
               " FunctionExpression": false
              }
            }
          ]
        }
}